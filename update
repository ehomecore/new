wget http://autoscriptnobita.tk/configdual7malluserpptp
mv ./alluserpptp /usr/bin/alluserpptp
chmod +x /usr/bin/alluserpptp

# instal Set Auto Reboot
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/userdeletepptp
mv ./userdeletepptp /usr/bin/userdeletepptp
chmod +x /usr/bin/userdeletepptp

# Install SPEED tES
apt-get install python
wget https://raw.githubusercontent.com/ehomecore/new/master/userdetail
mv ./userdetail /usr/bin/userdetail
chmod +x userdetail

# instal autolimitscript
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/userexpire
mv ./userexpire/usr/bin/userexpire
chmod +x /usr/bin/userexpire


# instal userdelete
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/usergenerate
mv ./usergenerate /usr/bin/usergenerate
chmod +x /usr/bin/usergenerate


# instal autolimitscript
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/userlimit
mv ./userlimit/usr/bin/userlimit
chmod +x /usr/bin/userlimit


# instal userdelete
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/userunlock
mv ./userunlock /usr/bin/userunlock
chmod +x /usr/bin/userunlock


# instal autolimitscript
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/userlog
mv ./userlog/usr/bin/userlog
chmod +x /usr/bin/userlog


# instal userdelete
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/userlogin
mv ./userlogin /usr/bin/userlogin
chmod +x /usr/bin/userlogin


# instal userdelete
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/userban
mv ./userban /usr/bin/userban
chmod +x /usr/bin/userban


# instal userdelete
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/userunlock
mv ./userunlock /usr/bin/userunlock
chmod +x /usr/bin/userunlock


# Install Menu
cd
wget https://raw.githubusercontent.com/ehomecore/new/master/menu
mv ./menu /usr/local/bin/menu
chmod +x /usr/local/bin/menu

